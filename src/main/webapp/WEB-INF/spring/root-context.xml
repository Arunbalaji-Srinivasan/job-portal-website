<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="userValidator"
		class="com.neu.jobsearch.validator.UserValidator"></bean>
	<bean id="userDao" class="com.neu.jobsearch.dao.UserDAO"></bean>

	<bean id="orgDao" class="com.neu.jobsearch.dao.OrganizationDAO"></bean>
	<bean id="organizationValidator"
		class="com.neu.jobsearch.validator.OrganizationValidator"></bean>

	<bean id="jobValidator"
		class="com.neu.jobsearch.validator.JobValidator"></bean>
	<bean id="jobDao" class="com.neu.jobsearch.dao.JobDAO"></bean>
</beans>
